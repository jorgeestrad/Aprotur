@model AproturWeb.Models.TipoDocumentoViewModel

@{
    ViewData["Title"] = "Tipo de Documento";
}

<h4>Nuevo Tipo de Documento</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form  enctype="multipart/form-data" asp-controller="Proyecto" asp-action="CreateTipoDocumento" method="post" class="mt-3">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="_TipoDocumento" />

            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-success">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}


}